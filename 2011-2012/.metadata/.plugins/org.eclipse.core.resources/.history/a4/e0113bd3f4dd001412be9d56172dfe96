import java.util.ArrayList;
import java.util.Arrays;

public class OneC {

    public static int hammingDist(String left, String right) {
        int dist = 0;
        for (int i = 0; i < left.length(); i++)	{
        	if (left.charAt(i) != right.charAt(i)){
        		dist++;
        	}
        }
        return dist;
    }

